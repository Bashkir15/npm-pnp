{
  "name": "npm-pnp",
  "scripts": {
    "prettier": "prettier --write \"src/**/*.js\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "git add"
    ]
  },
  "prettier": {
    "tabWidth": "4",
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "babel-preset-frost": "^0.1.0",
    "rollup-plugin-babel": "^3.0.2"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "prettier": "^1.8.2"
  }
}
